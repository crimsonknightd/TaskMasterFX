<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>

<DialogPane fx:controller="ctrl.NewUserController" xmlns="http://javafx.com/javafx/8.0.111"
	xmlns:fx="http://javafx.com/fxml/1">
	<content>
		<GridPane vgap="10" hgap="10">
			<Label text="メール アドレス" GridPane.rowIndex="0"
				GridPane.columnIndex="0" />
			<TextField fx:id="email" GridPane.rowIndex="0"
				GridPane.columnIndex="1" />
			<Label text="名前" GridPane.rowIndex="1"
				GridPane.columnIndex="0" />
			<TextField fx:id="name" GridPane.rowIndex="1"
				GridPane.columnIndex="1" />	
			

			<Label text="パスワード" GridPane.rowIndex="2"
				GridPane.columnIndex="0" />
			<PasswordField fx:id="password1" GridPane.rowIndex="2"
				GridPane.columnIndex="1" />
			
			<Label text="パスワード再入力" GridPane.rowIndex="3"
				GridPane.columnIndex="0" />
			<PasswordField fx:id="password2" GridPane.rowIndex="3"
				GridPane.columnIndex="1" />
				

			
		</GridPane>
	</content>
</DialogPane>
